#include<iostream>
using namespace std;
int main() {
	int a , b;
		cout << "enter a and b"<<endl;
		cin >> a >> b;
	char operation;
	cout << "enter sign";
	cin >> operation;
	switch (operation) {                                        //Calculator
	case '+': cout << "a+b " << (a + b);
		break;
	case  '-': cout << "a-b " << (a - b);
		break;
	case '*': cout << "a*b " << (a * b);
		break;
	case  '/' : cout << "a/b " << (a / b);
		break;
case  '%' : cout << "a%b " << (a % b);
		break;
	default: cout<<"invalid operator";
	}
}
